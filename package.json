{
  "name": "vector-electron-desktop",
  "version": "0.0.1",
  "description": "Basic electron wrapper for Vector (http://vector.im) a messaging client that uses Matrix protocol (http://matrix.org/).",
  "main": "app",
  "scripts": {
    "pack-icons": "iconutil -c icns icon.iconset",
    "pack-osx": "electron-packager . --out=dist --prune --asar --overwrite --platform=darwin --arch=x64",
    "pack-linux": "electron-packager . --out=dist --prune --asar --overwrite --platform=linux --arch=x64",
    "pack-win32": "electron-packager . --out=dist --prune --asar --overwrite --platform=win32 --arch=x64 --version-string.ProductName=\"Vector\" --version-string.FileDescription=\"Vector\" --version-string.OriginalFilename=\"Vector.exe\"",
    "start": "electron .",
    "zip-osx": "test -e dist/vector-electron-desktop-darwin-x64 && tar -czvf ./dist/vector-electron-darwin-x64.tgz dist/vector-electron-desktop-darwin-x64",
    "zip-linux": "test -e dist/vector-electron-desktop-linux-x64 && tar -czvf ./dist/vector-electron-linux-x64.tgz dist/vector-electron-desktop-linux-x64",
    "zip-windows": "test -e dist/vector-electron-desktop-win32-x64 && tar -czvf ./dist/vector-electron-win32-x64.tgz dist/vector-electron-desktop-win32-x64",
    "zip": "npm run zip-linux; npm run zip-osx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start.git"
  },
  "author": "KRISIS",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/electron/electron-quick-start/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start#readme",
  "devDependencies": {
    "devtron": "^1.2.1",
    "electron-packager": "^7.3.0",
    "electron-prebuilt": "^1.2.0"
  },
  "dependencies": {
    "open": "0.0.5"
  }
}
